<% if admin_users.empty? %>
  <p>No search results were returned.</p>
<% else %>
  <table class='table table-striped table-bordered'>
    <thead>
      <tr>
        <th>ID</th>
        <th>Email</th>
        <th>Created</th>
        <th>Last login</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% admin_users.each do |admin_user| %>
        <%= render 'line_item', admin_user: admin_user %>
      <% end %>
    </tbody>
  </table>

  <div class='pagination-container'>
    <%= pagination admin_users %>
  </div>
<% end %>
